        "exec": "$HOME/.config/waybar/calendar/exec.sh saturday"
    },

    "group/dates": {
        "orientation": "horizontal",
        "modules": [
            "group/sunday",
            "group/monday",
            "group/tuesday",
            "group/wednesday",
            "group/thursday",
            "group/friday",
            "group/saturday"
        ]
    },

