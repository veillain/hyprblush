{
    "layer": "top",
    "position": "left",
    "exclusive": false,
    "spacing": 0,

    "modules-left": [
        "custom/month",
        //"clock",
        "group/dates"
    ],
    "modules-center": [],
    "modules-right": [],

    "custom/month": {
        "format": "{}",
        "interval": 43200,
        "tooltip": false,
        "return-type": "bash",
        "exec": "$HOME/.config/waybar/calendar/exec.sh month"
    },
    "clock": {
        "format": "Today is {:%A}",
        "tooltip": false
    },

    "custom/sunday": {
        "format": "{}",
        "tooltip": false,
        "return-type": "json",
        "exec": "$HOME/.config/waybar/calendar/exec.sh sunday"
    },
    "custom/monday": {
        "format": "{}",
        "tooltip": false,
        "return-type": "json",
        "exec": "$HOME/.config/waybar/calendar/exec.sh monday"
    },
    "custom/tuesday": {
        "format": "{}",
        "tooltip": false,
        "return-type": "json",
        "exec": "$HOME/.config/waybar/calendar/exec.sh tuesday"
    },
    "custom/wednesday": {
