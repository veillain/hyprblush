{
  "layer": "top",
  "position": "top",
  // "height": 767,
  //"width": 1031,
  "spacing": 0,
  "margin-top": 0,
  "margin-bottom": 0,
  // "margin-left": -4,
  // "margin-right": -5,

  "modules-left": [ "clock", "hyprland/workspaces" ],
  "modules-center": [ "group/media" ],
  "modules-right": [
    // "custom/mediabar",
    "group/trayer",
    "custom/notification",
    "battery",
  ],

  "group/search": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 1000,
      "transition-left-to-right": true,
    },
    "modules": [
      "custom/search",
      "custom/terminal",
      "custom/browser",
      "custom/clipboard",
    ],
  },

  "custom/search": {
    "format": "Û∞£Ü",
    "tooltip": false,
    "on-click": "pkill -x rofi || rofi -no-steal-focus -show drun",
  },

  "custom/terminal": {
    "format": "Ôíâ",
    "tooltip-format": "Terminal",
    "on-click": "foot",
  },

  "custom/clipboard": {
    "format": "Û∞Öå",
    "tooltip-format": "Clipboard",
    "on-click": "$HOME/.config/bash/clipboard",
  },

  "custom/browser": {
    "format": "Û∞äØ",
    "tooltip-format": "Browser",
    "on-click": "google-chrome-stable",
  },

  "hyprland/workspaces": {
    "disable-scroll": true,
    // "all-outputs": true,
    "warp-on-scroll": false,
    "persistent-workspaces": {
        "*": [ 1,2,3,4,5 ]
    },
    "format": "{windows}",
    "format-window-separator": " ",
    "window-rewrite-default": "{name}",
    "window-rewrite": {
       "class<google-chrome>": "Ôâ®",
       "class<google-chrome> title<.*github.*>": "ÔÑì",
       "class<firefox>": "Û∞àπ",
       "class<firefox> title<.*github.*>": "ÔÑì",
       "class<kitty>": "ÓØä",
       "class<foot>": "Ôíâ",
       "class<pcmanfm>": "Ôìì",
       "class<vlc>": "Û∞ïº",
       "class<org.gnome.gThumb>": "ÔÄæ",
       "class<equibop>": "Ôáø",
       "title<.*youtube.*>": "Û∞óÉ",
       "title<.*NVIM.*>": "ÔçØ",
       "title<.*yazi.*": "Ôìì",
       "title<.*roller.*": "Ôìì",
       "title<.*monkeytype.*": "Û∞åå",
       "title<.*volume.*": "Óò∏",
       "title<.*discord.*": "Ôáø",
       "title<.*legcord.*": "Ôáø",
       "title<.*obsidian.*": "Û∞éû",
       "title<.*obs.*": "ÔÄΩ",
       "title<.*spotify.*": "Û∞ìá",
       "title<.*telegram.*": "Óàó",
       "code": "<U+F0A1E>",
       "title<.* - (.*) - VSCodium>": "codium $1"  // captures part of the window title and formats it into output
       // ".*": "ÔëÑ"
    }
  },

  "dwl/tags": {
    "num-tags": 9
  },

  "group/options": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 1000,
      "transition-left-to-right": true,
    },
    "modules": [
      "custom/options",
      "group/volume",
      "group/brightness",
      "group/packages",
    ],
  },

  "custom/options": {
    "format": "ÔÄì",
    "tooltip": false,
    "on-click": "$HOME/.config/waybar/home/toggle.sh",
  },

  "group/volume": {
    "orientation": "inherit",
    "modules": ["custom/volume", "pulseaudio"],
    "on-scroll-up": "$HOME/.config/bash/volume --up",
    "on-scroll-down": "$HOME/.config/bash/volume --down",
  },

  "group/brightness": {
    "orientation": "inherit",
    "modules": ["custom/brightness", "backlight"],
    "on-scroll-up": "$HOME/.config/bash/brightness --up",
    "on-scroll-down": "$HOME/.config/bash/brightness --down",
  },

  "group/packages": {
    "orientation": "inherit",
    "modules": ["custom/packages", "custom/packagesnum"],
  },

  "custom/brightness": {
    "format": "Û∞Éû",
    "tooltip": false,
  },

  "custom/volume": {
    "format": "ÔÄ®",
    "tooltip": false,
  },

  "custom/packages": {
    "format": "{icon}",
    "return-type": "json",
    "format-icons": {
      "has-updates": "Û±ßò",
      "updated": "Û∞èñ",
    },
    "exec": "waybar-module-pacman-updates --interval-seconds 5 --network-interval-seconds 300",
    "on-click": "foot sudo pacman -Syu",
  },

  "custom/packagesnum": {
    "format": "{}",
    "format-updated": "{icon}",
    "format-icons": {
      "has-updates": "{}",
      "updated": "ÔÄå",
    },
    "return-type": "json",
    "exec": "waybar-module-pacman-updates --interval-seconds 5 --network-interval-seconds 300",
  },

  "pulseaudio": {
    "scroll-step": 5, // %, can be a float
    "format": "{volume}",
    "format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
    "format-bluetooth-muted": "Ôö© {icon}Ôäî {format_source}",
    "format-muted": "Ôö©",
    "format-source": "{volume}% ÔÑ∞",
    "format-source-muted": "ÔÑ±",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "Ôñê",
      "headset": "Ôñê",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"],
    },
    "on-click": "pavucontrol",
    "on-scroll-up": "$HOME/.config/bash/volume --up",
    "on-scroll-down": "$HOME/.config/bash/volume --down",
  },

  "backlight": {
    "scroll-step": 5,
    "format": "{percent}",
    "tooltip": false,
    "format-icons": ["üåë", "üåò", "üåó", "üåñ", "üåï"],
    "on-scroll-up": "$HOME/.config/bash/brightness --up",
    "on-scroll-down": "$HOME/.config/bash/brightness --down"
  },

  "custom/newworkspaces": {
    "format": "+",
    "tooltip-format": "Create New Workspace",
    "on-click": "hyprctl dispatch workspace +1",
  },

  // ---- Center ----
  "clock": {
    "format": "{:%H.%M}",
    // "format-alt": "{:%a, %d %b - %H.%M}",
    "format-alt": "{:%A, %d %B - %H.%M}",
    "tooltip": true,
    "tooltip-format": "{:%A, %d %B %Y}",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      //"weeks-pos"     : "left",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>",
      },
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down",
    },
    "on-click": "$HOME/.config/waybar/calendar/exec.sh",
  },

    "group/media": {
        "orientation": "inherit",
        "modules": [
            "image",
            "custom/images",
            "group/mediatext"
        ]
    },
    "image": {
        "path": "/tmp/mediathumbnail",
        "size": 24,
        "signal": 4,
        "interval": 5,
        "on-click": "gthumb /tmp/mediathumbnail",
    },
    "custom/images": {
        "format": "{}",
        "tooltip": false,
        "exec": "$HOME/.config/waybar/normal/media.sh image"
    },
    "group/mediatext": {
        "orientation": "vertical",
        "modules": [
            "group/titles",
            "group/artists"
        ]
    },
    "group/titles": {
        "orientation": "horizontal",
        "modules": [
            "custom/title"
        ]
    },
    "group/artists": {
        "orientation": "horizontal",
        "modules": [
            "custom/artist"
        ]
    },
    "custom/title": {
        "format": "{}",
        "return-type": "json",
        "exec": "$HOME/.config/waybar/normal/media.sh title",
        "tooltip": false,
        "on-click": "playerctl play-pause"
    },
    "custom/artist": {
        "format": "{}",
        "return-type": "json",
        "exec": "$HOME/.config/waybar/normal/media.sh artist",
        "tooltip": false,
        "on-click": "playerctl play-pause"
    },



  // ---- Bottom ----
  "custom/mediabar": {
    "format": "ÔÄÅ",
    "tooltip": false,
    "on-click": "$HOME/.config/bash/mediabar-toggle",
    "on-click-right": "playerctl play-pause",
  },

  "group/trayer": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 500,
      "transition-left-to-right": false,
    },
    "modules": ["custom/tray", "tray"],
  },
  "custom/tray": {
    "format": "ÔÅì",
    "tooltip": false,
  },
  "tray": {
    "icon-size": 18,
    "spacing": 3,
  },

  "custom/notification": {
    "tooltip": false,
    "format": "{icon}",
    "format-icons": {
      "notification": "Û±Ö´",
      "none": "Û∞Çú",
      "dnd-notification": "Û∞Ç†",
      "dnd-none": "Û∞™ì",
      "inhibited-notification": "Û±èß",
      "inhibited-none": "Û±è®",
      "dnd-inhibited-notification": "Û∞Çõ",
      "dnd-inhibited-none": "Û∞™ë",
    },
    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "swaync-client -swb",
    "on-click": "swaync-client -t -sw",
    "on-click-right": "swaync-client -d -sw",
    "escape": true,
  },

  "battery": {
    "states": {
      // "good": 95,
      "warning": 30,
      "critical": 15,
    },
    "format": "{icon}",
    "format-full": "{icon}",
    "format-charging": "ÔÉß",
    "format-plugged": "Ôá¶  {capacity}%",
    // "format-alt": "{icon}  {time}",
    // "format-good": "", // An empty format will hide the module
    // "format-full": "",
    "format-icons": ["Û∞Çé", "Û∞Å∫", "Û∞Åª", "Û∞Åº", "Û∞ÅΩ", "Û∞Åæ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞ÇÇ", "Û∞Åπ"],
    "on-click": "bash $HOME/.config/waybar/power/toggle.sh"
  },

  "mpd": {
    "format": "{stateIcon} {consumeIcon}{randomIcon}{repeatIcon}{singleIcon}{artist} - {album} - {title} ({elapsedTime:%M:%S}/{totalTime:%M:%S}) ‚∏®{songPosition}|{queueLength}‚∏© {volume}% ÔÄÅ",
    "format-disconnected": "Disconnected ÔÄÅ",
    "format-stopped": "{consumeIcon}{randomIcon}{repeatIcon}{singleIcon}Stopped ÔÄÅ",
    "unknown-tag": "N/A",
    "interval": 5,
    "consume-icons": {
      "on": "ÔÉÑ ",
    },
    "random-icons": {
      "off": "<span color=\"#f53c3c\">ÔÅ¥</span> ",
      "on": "ÔÅ¥ ",
    },
    "repeat-icons": {
      "on": "ÔÄû ",
    },
    "single-icons": {
      "on": "ÔÄû1 ",
    },
    "state-icons": {
      "paused": "ÔÅå",
      "playing": "ÔÅã",
    },
    "tooltip-format": "MPD (connected)",
    "tooltip-format-disconnected": "MPD (disconnected)",
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ÔÅÆ",
      "deactivated": "ÔÅ∞",
    },
  },
  "cpu": {
    "format": "{usage}% Ôãõ",
    "tooltip": false,
  },
  "memory": {
    "format": "{}% ÔÉâ",
  },
  "temperature": {
    // "thermal-zone": 2,
    // "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
    "critical-threshold": 80,
    // "format-critical": "{temperatureC}¬∞C {icon}",
    "format": "{temperatureC}¬∞C {icon}",
    "format-icons": ["Ôù´", "Ôãâ", "Ôù©"],
  },

  "battery#bat2": {
    "bat": "BAT2",
  },
  "power-profiles-daemon": {
    "format": "{icon}",
    "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
    "tooltip": true,
    "format-icons": {
      "default": "ÔÉß",
      "performance": "ÔÉß",
      "balanced": "Ôâé",
      "power-saver": "ÔÅ¨",
    },
  },
  "network": {
    "format-wifi": " Ntw : {signalStrength}% ",
    "tooltip-format": "{essid} ({signalStrength}%) Ôá´",
    "format-ethernet": "{ipaddr}/{cidr} Ôûñ",
    // "tooltip-format": "{ifname} via {gwaddr} Ôûñ",
    "format-linked": "{ifname} (No IP) Ôûñ",
    "format-disconnected": "Disconnected ‚ö†",
    "format-alt": "{ifname}: {ipaddr}/{cidr}",
  },
}
